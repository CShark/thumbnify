                                Fill="{StaticResource DigitBrush}"
                                Stretch="Fill" />
                        </Canvas>
                    </ControlTemplate.Resources>
                    <Border Background="{StaticResource ControlBackgroundBrush}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="5">
                        <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center" Stretch="Uniform">
                            <Grid Name="PART_ClockGrid" Margin="5,10,5,10" />
